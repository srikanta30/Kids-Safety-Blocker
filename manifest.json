{
    "name": "Kid's Safety Blocker",
    "description": "Keep Your Kids Safe While They Browse The Internet.",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["http://*/*",
                "https://*/*"],
            "js": ["blocker.js"]
        }
    ],
    "permissions": [
        "activeTab"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "images/icon.png"
      }
  }